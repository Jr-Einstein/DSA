#include<iostream>
using namespace std;

int main(){

    int n;
    cin>>n;

    int i=2;
    while(i<n){
        if(n%i==0){
            cout<<"The Number is Prime Number for"<<i<<endl;
        }
        else{
            cout<<"The Number is Not Prime Number for "<<i<<endl;
        }
        i=i+1;
    }
}
